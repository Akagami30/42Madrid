#include <stdio.h>
void	ft_sort_int_tab(int *tab, int size)
{
	int	vswitch;
	int	i;
	
	i = 0;
	while(size >= 0)
	{
		vswitch = 0;
		if(tab[i] > tab[i + i])
		{
			vswitch = tab[1];
			tab[i] = tab[i + 1];
			tab[i + 1] = vswitch;		
		}
		i++;
	}
	size--;
}

int	main(void)
{
	int	tab;
	int	size;
	tab[6] = 2, 3, 1, 6, 8, 4, 9};
	size = 7;
	ft_sort_int_tab(tab);
	printf(tab);
	return(0);
}
